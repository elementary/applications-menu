#: data/applications-menu.appdata.xml.in:6
msgid "Applications Menu"
msgstr "Nabídka s aplikacemi"

#: data/applications-menu.appdata.xml.in:7
msgid "Open and search for apps"
msgstr "Spouštějte a hledejte aplikace"

#: data/applications-menu.appdata.xml.in:11
#: data/applications-menu.appdata.xml.in:23
#: data/applications-menu.appdata.xml.in:47
msgid "Improvements:"
msgstr "Vylepšení:"

#: data/applications-menu.appdata.xml.in:13
msgid "Searching settings always searches the full title"
msgstr "Hledání nastavení vždy vyhledá úplný nadpis"

#: data/applications-menu.appdata.xml.in:19
#: data/applications-menu.appdata.xml.in:35
msgid "Fixes:"
msgstr "Opravy:"

#: data/applications-menu.appdata.xml.in:21
msgid "Fix a regression with settings search"
msgstr "Opravena regrese ve vyhledávání v nastaveních"

#: data/applications-menu.appdata.xml.in:25
#: data/applications-menu.appdata.xml.in:41
#: data/applications-menu.appdata.xml.in:50
#: data/applications-menu.appdata.xml.in:62
#: data/applications-menu.appdata.xml.in:71
#: data/applications-menu.appdata.xml.in:80
#: data/applications-menu.appdata.xml.in:94
#: data/applications-menu.appdata.xml.in:102
#: data/applications-menu.appdata.xml.in:114
#: data/applications-menu.appdata.xml.in:123
#: data/applications-menu.appdata.xml.in:132
msgid "Updated translations"
msgstr "Aktualizovány překlady"

#: data/applications-menu.appdata.xml.in:31
#: data/applications-menu.appdata.xml.in:56
msgid "New features:"
msgstr "Nové funkce:"

#: data/applications-menu.appdata.xml.in:33
msgid "Show search results for bookmarked folders"
msgstr "Zobrazovány výsledky vyhledávání pro složky, zařazené do záložek"

#: data/applications-menu.appdata.xml.in:37
msgid "Prevent a potential crash on startup"
msgstr "Zabráněno potenciálnímu pádu při spouštění"

#: data/applications-menu.appdata.xml.in:39
#: data/applications-menu.appdata.xml.in:60
#: data/applications-menu.appdata.xml.in:68
#: data/applications-menu.appdata.xml.in:77
#: data/applications-menu.appdata.xml.in:86
#: data/applications-menu.appdata.xml.in:100
#: data/applications-menu.appdata.xml.in:108
msgid "Minor updates:"
msgstr "Drobné aktualizace:"

#: data/applications-menu.appdata.xml.in:49
msgid "Show secondary click menu for search results"
msgstr ""
"U výsledků vyhledávání zobrazovat kontextovou nabídku (pravé tlačítko myši)"

#: data/applications-menu.appdata.xml.in:58
msgid ""
"Launch apps selectively on integrated or dedicated graphics for systems with "
"hybrid graphics"
msgstr ""
"Selektivní spouštění aplikací na grafickém čipu v procesoru nebo tom "
"samostatném u systémů s dvojitou grafikou"

#: data/applications-menu.appdata.xml.in:70
msgid "Show apps with no category under \"Other\""
msgstr "Aplikace, nezařazené do žádné kategorie, do „Ostatní“"

#: data/applications-menu.appdata.xml.in:79
msgid "Fix an issue where Flatpak apps wouldn't show until after a restart"
msgstr ""
"Opraven problém kdy se do restartu nezobrazovaly aplikace pocházející z "
"Flatpak balíčků"

#: data/applications-menu.appdata.xml.in:88
msgid "Sort app actions above AppCenter search"
msgstr "Seřadit akce aplikace nad hledáním v centru aplikací"

#: data/applications-menu.appdata.xml.in:89
msgid "Always hide Terminal-based programs"
msgstr "Programy určené pro Terminál jsou vždy skryty"

#: data/applications-menu.appdata.xml.in:90
msgid "Remove document viewer from block list"
msgstr "Ze seznamu blokovaných odebrán prohlížeč dokumentů"

#: data/applications-menu.appdata.xml.in:91
msgid "Support fractions without leading number in calculator"
msgstr "V kalkulačce jsou podporována desetinná čísla bez uvozujících čísel"

#: data/applications-menu.appdata.xml.in:92
msgid "Show overlay key in tooltip when set"
msgstr "Pokud je nastavena, zobrazovat překryvnou klávesu v bublinové nápovědě"

#: data/applications-menu.appdata.xml.in:93
#: data/applications-menu.appdata.xml.in:122
#: data/applications-menu.appdata.xml.in:131
msgid "Performance improvements"
msgstr "Vylepšení výkonu"

#: data/applications-menu.appdata.xml.in:110
msgid ""
"Smoother navigation gesture in grid view and touchscreen gesture support"
msgstr ""
"Hladší gesta pohybu v zobrazení v mřížce a podpora gest dotykové obrazovky"

#: data/applications-menu.appdata.xml.in:111
msgid "Focus search when navigating up from the category view"
msgstr ""
"Zaměření kolonky pro vyhledávání při pohybu nahoru ze zobrazení s kategoriemi"

#: data/applications-menu.appdata.xml.in:112
msgid "Don't focus search when pressing modifier keys"
msgstr "Nezaměřování kolonky pro vyhledávání při stisku kláves-modifikátorů"

#: data/applications-menu.appdata.xml.in:113
msgid "Ensure categories are sorted alphabetically"
msgstr "Zajištění, že jsou všechny kategorie řazeny abecedně"

#: data/applications-menu.appdata.xml.in:120
msgid "Category view is now a list"
msgstr "Zobrazení s kategoriemi je nyní seznam"

#: data/applications-menu.appdata.xml.in:121
msgid "Improved keyboard navigation"
msgstr "Vylepšená navigace pomocí kláves"

#: data/applications-menu.appdata.xml.in:128
msgid ""
"Improved keyboard navigation, including new tooltips and menu key support"
msgstr ""
"Vylepšená navigace pomocí klávesnice, včetně nových nástrojových tipů a "
"podpory klávesy menu"

#: data/applications-menu.appdata.xml.in:129
msgid "Highlight action matches in the search view"
msgstr "Zvýraznění akce shodující se v zobrazení hledání"

#: data/applications-menu.appdata.xml.in:130
msgid "Add a \"View in AppCenter\" menu item"
msgstr "Přidána položka nabídky „Zobrazit v Centru aplikací“"

#: data/applications-menu.appdata.xml.in:144
msgid "elementary, Inc."
msgstr "elementary, Inc."

